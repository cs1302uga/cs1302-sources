<!doctype html>
#parse( "site-macros.vm" )
<!--
 | Generated by #generatedBy().
 | Rendered using cs1302-site-skin.
-->
<html#if ( $locale ) lang="$locale.language"#end>
  <head>
    <meta charset="${outputEncoding}">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="generator" content="#generatedBy()" />
#foreach( $author in $authors )
    <meta name="author" content="$author" />
#end
#if ( $documentDate )
    <meta name="date" content="$documentDate" />
#end
    <meta name="theme-color" content="#7952b3">
#if ( $title )
    <title>$title</title>
#end
    <title>Hello, world!</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/highlightjs/cdn-release@11.5.1/build/styles/default.min.css">
  </head>
  <body class="d-flex flex-column h-100">
    <header>
      <nav class="navbar navbar-expand-md navbar-dark bg-dark">
        <div class="container">
          <a class="navbar-brand" href="#">$project.artifactId</a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarCollapse">
            <div class="navbar-nav">
              #**##link( $link.relative("index.html") "About" "nav-link active" )
              #**##link( $link.relative("project-info.html") "Project Information" "nav-link" )
              #**##link( $link.relative("project-reports.html") "Generated Reports" "nav-link" )
            </div>
          </div>
        </div>
      </nav>
    </header>
    <aside class="bg-light">
      <div class="container py-2">
        <img alt="Maven Central" src="https://img.shields.io/maven-central/v/$project.groupId/$project.artifactId">
      </div>
    </aside>
    <main class="flex-shrink-0">
      <div class="container mt-3">
        $bodyContent
      </div>
    </main>
    <footer class="footer mt-3 py-3 border-top">
      <div class="container">
#*  *##if( $decoration.body.footer )
        $render.eval( $decoration.body.footer )
#*  *##else
        Copyright &#169;#copyright().#publishDate( "bottom" $decoration.publishDate $decoration.version )
#*  *##end
      </container>
    </footer>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/gh/highlightjs/cdn-release@11.5.1/build/highlight.min.js"></script>
    <script>
    document.addEventListener('DOMContentLoaded', (event) => {
        document.querySelectorAll('.source pre:not([class*="language"])').forEach((el) => {
            el.classList.add('language-plaintext');
        });
        document.querySelectorAll('.source pre').forEach((el) => {
            hljs.highlightElement(el);
        });
    });
    </script>
  </body>
</html>
