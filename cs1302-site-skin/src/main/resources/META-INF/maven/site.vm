<!doctype html>
#parse( "site-macros.vm" )
<!--
 | Generated by #generatedBy().
 | Rendered using cs1302-site-skin.
-->
<html#if ( $locale ) lang="$locale.language"#end>
  <head>
    <meta charset="$outputEncoding">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="generator" content="#generatedBy()" />
#foreach( $author in $authors )
    <meta name="author" content="$author" />
#end
#if ( $documentDate )
    <meta name="date" content="$documentDate" />
#end
    <meta name="theme-color" content="#212529">
#if ( $title )
    <title>$title</title>
#end
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link href="https://cdn.jsdelivr.net/gh/highlightjs/cdn-release@11.5.1/build/styles/default.min.css" rel="stylesheet">
    <link href="$relativePath/css/cs1302-site-skin.css" rel="stylesheet">
    <link href="$relativePath/css/site.css" rel="stylesheet" />
    <link href="$relativePath/css/print.css" rel="stylesheet" media="print" />
  </head>
  <body class="d-flex flex-column h-100">
    <header>
      <nav class="navbar navbar-expand-md navbar-dark bg-dark">
        <div class="container align-items-center">
          <a class="navbar-brand flex-fill" href="$project.url">
            <span id="artifact-id">$project.artifactId</span>
            <small id="project-version" class="fs-6 text-white-50 d-inline-block">$project.version</small>
          </a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse justify-content-end" id="navbarCollapse">
            <div class="navbar-nav">
            #foreach( $menu in $decoration.body.menus )
                #if ( $menu.name )
                    #if ( $menu.items.isEmpty() )
                        <!-- $menu.name menu is present but has no items -->
                        <a class="nav-link disabled" href="#">$menu.name</a>
                    #else
                        <div class="nav-item dropdown">
                          <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">$menu.name</a>
                          <div class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdownMenuLink">
                          #foreach ( $item in $menu.items )
                            <a class="dropdown-item" href="#calcLink ( $item.href )">$item.name</a>
                          #end
                          </div>
                        </div>
                    #end
                #else
                    #foreach ( $item in $menu.items )
                        <a class="nav-link" href="#calcLink ( $item.href )">$item.name</a>
                    #end
                #end
            #end
            </div>
          </div>
        </div>
      </nav>
    </header>
    <aside class="bg-light">
      <div class="container py-2 d-flex gap-2">
        #mavenCentralBadge( $project.groupId $project.artifactId )
        #if ( $project.packaging != "pom" )#ossrhSnapshotBadge#end
      </div>
    </aside>
    <div class="flex-shrink-0">
      <div class="container mt-3">
        <nav aria-label="breadcrumb">
          <ol class="breadcrumb">
#*        *##foreach ( $item in $decoration.body.breadcrumbs )
              <li class="breadcrumb-item active" aria-current="page">
#*            *##if( $item.href != $currentFileName )
                <a href="#calcLink ( $item.href )">$item.name</a>
#*            *##else
                $item.name
#*            *##end
              </li>
#*        *##end
          </ol>
        </nav>
        <main class="bodyContent">
#set ( $body = $bodyContent )
#set ( $body = $body.replaceAll( "class=${esc.q}bodyTable${esc.q}", "class=${esc.q}table table-striped table-borderless${esc.q}" ) )
#set ( $body = $body.replaceAll( "class=${esc.q}bodyTable ", "class=${esc.q}table table-striped table-borderless " ) )
#*        *#$body
        </main>
      </div>
    </div>
    <footer class="footer mt-3 py-3 border-top">
      <div class="container">
#*  *##if( $decoration.body.footer )
        $render.eval( $decoration.body.footer )
#*  *##else
        Copyright &#169;#copyright().#publishDate( "bottom" $decoration.publishDate $decoration.version )
#*  *##end
      </container>
    </footer>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/gh/highlightjs/cdn-release@11.5.1/build/highlight.min.js"></script>
    <script>
    document.addEventListener('DOMContentLoaded', (event) => {
        document.querySelectorAll('.source pre:not([class*="language"])').forEach((el) => {
            el.classList.add('language-plaintext');
        });
        document.querySelectorAll('.source pre').forEach((el) => {
            hljs.highlightElement(el);
        });
    });
    </script>
  </body>
</html>
