<project xmlns="http://maven.apache.org/DECORATION/1.8.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.8.0 http://maven.apache.org/xsd/decoration-1.8.0.xsd"
          name="CSCI 1302 API Tools">

  <version position="right"/>
  <publishDate position="right"/>

  <skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>1.9</version>
  </skin>

  <custom>
    <fluidoSkin>
      <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
    </fluidoSkin>
  </custom>

  <bannerLeft>
    <name>${project.organization.name}</name>
  </bannerLeft>

  <body>

    <breadcrumbs>
      <item name="${project.name}" href="${this.url}"/>
    </breadcrumbs>

    <menu name="Overview">
      <item name="Welcome" href="index.html" />
    </menu>

    <menu name="API Documentation">
      <item name="User API" href="user-api/apidocs/index.html" />
      <item name="Developer API" href="devel-api/apidocs/index.html" />
    </menu>

    <menu ref="reports" />
    <menu ref="modules" />
    <menu ref="parent" />

    <footer>
      <![CDATA[

        <p>
          &copy; ${this.inceptionYear} <a href="${this.organization.url}">${this.organization.name}</a>.
        </p>
        <p class="text-muted">
          The content and opinions expressed on this Web page do not necessarily reflect the views of nor are they endorsed by the University of Georgia or the University System of Georgia.
        </p>

      ]]>
    </footer>

  </body>

</project>
